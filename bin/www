#!/usr/bin/env node
const app = require('../app');
const db = require('../db/models')
const { port } = require('../config');

db.sequelize.authenticate()
  .then((res) => {
    console.log("database connection success! ğŸ¤©")
    app.listen(port, () => {
      console.log(`server listening at ${port}`);
    })
  })
  .catch(err =>{
    console.log("database connection failure ğŸ˜­");
    console.error(err);
  });
